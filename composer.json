{
    "name": "hlacos/lara-mvcms",
    "description": "Simple core CMS Admin written in Laravel",
    "keywords": [
        "league",
        "lara-mvcms",
        "admin",
        "cms"
    ],
    "homepage": "https://github.com/Hlacos/LaraMvcms",
    "license": "MIT",
    "authors": [
        {
            "name": "Heiszmann László",
            "email": "heiszmann@hlacos.hu",
            "homepage": "http://hlacos.hu",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "kbwebs/multiauth": "~1.0",
        "spatie/laravel-analytics": "^1.2",
        "dimsav/laravel-translatable": "^5.4",
        "hlacos/attachment5": "dev-master",
        "baum/baum": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Hlacos\\LaraMvcms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hlacos\\LaraMvcms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-stable"
        }
    }
}
